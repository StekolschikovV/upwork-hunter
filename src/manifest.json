{
  "manifest_version": 3,
  "name": "Upwork hunter",
  "description": "Upwork job search assistant extension.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://*.upwork.com/*",
    "*://*.openai.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "content.styles.css",
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": []
    }
  ]
}
